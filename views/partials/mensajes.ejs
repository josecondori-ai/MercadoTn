<% if(success_msg && success_msg !=''){%>
    <div class="alert alert-success" role="alert">
       <%=success_msg%>
      </div>
      
<%}else if(error_msg && error_msg !=''){%>
    <div class="alert alert-danger" role="alert">
        <%=error_msg%>
       </div>
   <% }%>


   <%if(error && error !=''){%>
    <div class="alert alert-danger" role="alert">
        <%=error%>
       </div>

   <%}%>